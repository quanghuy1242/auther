                {
                    "name": "v",
                    "type": "any",
                    "description": "Value to assert"
                },
                {
                    "name": "message",
                    "type": "string",
                    "optional": true,
                    "description": "Error message if assertion fails"
                }
            ],
            "returns": {
                "type": "any",
                "description": "Returns v if truthy"
            }
        },
